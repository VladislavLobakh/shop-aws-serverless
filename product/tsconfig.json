{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2017"],

    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "ts-temp",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "preserveConstEnums": true,
    "isolatedModules": true,
    "removeComments": true,
    "resolveJsonModule": true
  }
}

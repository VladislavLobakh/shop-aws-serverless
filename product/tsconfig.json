{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ESNext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "ts-temp",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "types": ["node", "jest"]
  },
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
}
